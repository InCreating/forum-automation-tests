<suite name="Authorization page tests" verbose="1">
    <test name="Chrome Login Test">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="LoginPage.LogInTest">
                <methods>
                    <include name="loginChrome"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Edge Login Test">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="LoginPage.LogInTest">
                <methods>
                    <include name="loginEdge"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>
