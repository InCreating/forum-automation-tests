<suite name="Links redirection" verbose="1">
    <test name="Chrome redirection">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="CommentPage.CommentTest">
                <methods>
                    <include name="createComment"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Edge redirection">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="CommentPage.CommentTest">
                <methods>
                    <include name="createComment"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>
